{
	"cmd": "dub",
	"args": [
		"build"
	],
	"name": "Build Default",
	"sh": false,
	"targets": {
		"Run Default": {
			"cmd": "dub",
			"sh": false,
			"args": [
				"run"
			]
		},
		"Build lib": {
			"cmd": "dub",
			"sh": false,
			"args": [
				"build",
				"--config=lib"
			]
		},
		"Build Demo": {
			"cmd": "dub",
			"sh": false,
			"args": [
				"build",
				"--config=Demo"
			]
		},
		"Run Demo": {
			"cmd": "dub",
			"sh": false,
			"args": [
				"run",
				"--config=Demo"
			]
		}
	}
}